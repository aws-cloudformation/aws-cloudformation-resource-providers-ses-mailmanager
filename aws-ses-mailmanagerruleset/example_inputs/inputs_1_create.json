{
    "Description": "description",
    "RuleSetName": "rule_set_name",
    "Rules": [
        {
            "Name": "rule_one",
            "Conditions": [
                {
                    "StringExpression": {
                        "Evaluate": {
                            "Attribute": "MAIL_FROM"
                        },
                        "Operator": "CONTAINS",
                        "Values": [
                            "@amazon.com"
                        ]
                    }
                }
            ],
            "Unless": [
                {
                    "StringExpression": {
                        "Evaluate": {
                            "Attribute": "MAIL_FROM"
                        },
                        "Operator": "CONTAINS",
                        "Values": [
                            "@badsender.com"
                        ]
                    }
                }
            ],
            "Actions": [
                {
                    "WriteToS3": {
                        "S3Bucket": "s3_bucket_name",
                        "S3Prefix": "folder",
                        "S3SseKmsKeyId": "kms_id"
                    }
                }
            ]
        }
    ]
}
